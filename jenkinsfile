node ("maven") {
   stage('Preparation') { // for display purposes
      // Get some code from a GitHub repository
      git 'https://github.com/hbenmansour/openshift-jmeter-kafka.git'
               
   }
   stage('Build') {
      // Run the maven build
      sh "mvn clean install"
    
   }
 
   stage('Load Test') {
     sh "cd kafka-bench && mvn verify"
   } 
   
}
